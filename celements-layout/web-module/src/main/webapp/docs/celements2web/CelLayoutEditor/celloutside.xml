<guid>cd8789bb-fcfc-4541-8ba8-7c377440f5a3</guid>
</object>
<content>&lt;h2&gt;Cells outside the structure&lt;/h2&gt;
#set($xwql = "where doc.space = '${doc.space}'")
#set($outCellFound = false)
#foreach($fullName in $services.query.xwql($xwql).execute())
  #set($cur_doc = $xwiki.getDocument($fullName))
  #set($menu_items = $cur_doc.getObjects("Celements2.MenuItem"))
  #if(($menu_items.size() == 0) &amp;&amp; ($cur_doc.getName() != "WebPreferences") &amp;&amp; ($cur_doc.getName() != "WebHome"))
#if(!$outCellFound)
&lt;ul class="cel_layout_editor_outside"&gt;
#set($outCellFound = true)
#end
&lt;li&gt;
  #set($docRef = $services.model.resolveDocument("$fullName"))
  &lt;a href="$!services.url.getURL($docRef, 'view')" target="_blank"&gt;$!docRef.name&lt;/a&gt;
  &lt;a href="$!services.url.getURL($docRef, 'delete')" target="_blank"&gt;&lt;img src="/file/resources/icons/silk/cross.gif" alt="delete"&gt;&lt;/a&gt;
&lt;/li&gt;
  #end
#end
#if(!$outCellFound)
&lt;p&gt;No cells found!&lt;/p&gt;
#else
&lt;/ul&gt;
#end</content></xwikidoc>
